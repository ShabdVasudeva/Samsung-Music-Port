<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="true"
        android:gravity="center_vertical"
        android:height="@dimen/sesl_seekbar_mode_expand_track_height">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/sesl_seekbar_level_control_color_default" />
            <corners
                android:radius="@dimen/sesl_seekbar_level_progress_radius" />
            <size
                android:height="@dimen/sesl_seekbar_mode_expand_track_height" />
        </shape>
    </item>
</layer-list>