<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:gravity="center">
        <shape
            android:shape="oval">
            <size
                android:height="16.0dip"
                android:width="16.0dip" />
            <solid
                android:color="@color/sesl_switch_thumb_off_color" />
        </shape>
    </item>
    <item
        android:gravity="center">
        <shape
            android:shape="oval">
            <size
                android:height="20.0dip"
                android:width="20.0dip" />
            <stroke
                android:width="2.0dip"
                android:color="?switchOffStrokeColor" />
        </shape>
    </item>
</layer-list>