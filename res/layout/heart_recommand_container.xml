<?xml version="1.0" encoding="utf-8"?>
<com.samsung.android.app.musiclibrary.ui.widget.RecyclerViewItemLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    app:fitHeight="true"
    app:fitSpreadId="@id/no_item_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.samsung.android.app.musiclibrary.ui.widget.OneUiConstraintLayout
        android:id="@id/no_item_layout"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/mu_list_item_space_outer_default"
        android:layout_marginEnd="@dimen/mu_list_item_space_outer_default"
        app:layout_constraintBottom_toTopOf="@id/divider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:id="@id/main_text"
            android:layout_width="0.0dip"
            android:text="@string/no_favorites"
            app:layout_constraintBottom_toTopOf="@id/sub_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed"
            style="@style/Font.NewSEC.400.NoItem.Main" />
        <TextView
            android:id="@id/sub_text"
            android:layout_width="0.0dip"
            android:layout_marginTop="@dimen/mu_no_item_sub_text_margin_top"
            android:text="@string/favorite_help_view_guide"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/main_text"
            style="@style/Font.NewSEC.400.NoItem.Sub" />
    </com.samsung.android.app.musiclibrary.ui.widget.OneUiConstraintLayout>
    <View
        android:id="@id/divider"
        android:background="@color/basics_list_divider"
        android:layout_width="0.0dip"
        android:layout_height="@dimen/mu_list_divider_height_winset"
        android:layout_marginStart="@dimen/mu_list_item_space_outer_default"
        android:layout_marginEnd="@dimen/mu_list_item_space_outer_default"
        app:layout_constraintBottom_toTopOf="@id/recommend_container"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/no_item_layout" />
    <com.samsung.android.app.musiclibrary.ui.widget.OneUiConstraintLayout
        android:id="@id/recommend_container"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/heart_recommend_container_min_height"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider">
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:id="@id/guideline_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_begin="@dimen/mu_list_item_space_outer_default" />
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:id="@id/guideline_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_end="@dimen/mu_list_item_space_outer_default" />
        <ImageView
            android:id="@id/icon"
            android:layout_width="@dimen/heart_recommend_title_icon_size"
            android:layout_height="@dimen/heart_recommend_title_icon_size"
            android:tintMode="src_in"
            app:layout_constraintBottom_toTopOf="@id/barrier"
            app:layout_constraintStart_toEndOf="@id/guideline_start"
            app:layout_constraintTop_toTopOf="parent"
            app:tint="@color/heart_recommend_header_icon" />
        <TextView
            android:textSize="@dimen/heart_spotify_title_kt"
            android:textColor="@color/heart_recommend_header_text"
            android:gravity="center_vertical"
            android:id="@id/title"
            android:visibility="gone"
            android:minHeight="@dimen/heart_recommend_title_min_height"
            android:includeFontPadding="false"
            android:layout_marginStart="@dimen/heart_recommend_title_margin_start"
            app:layout_constraintBottom_toTopOf="@id/barrier"
            app:layout_constraintStart_toEndOf="@id/icon"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/Font.NewSEC.600" />
        <ImageView
            android:id="@id/more"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@id/barrier"
            app:layout_constraintEnd_toStartOf="@id/guideline_end"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/ListItem.More" />
        <androidx.constraintlayout.widget.Barrier
            android:id="@id/barrier"
            android:layout_width="wrap_content"
            android:layout_height="0.0dip"
            app:barrierDirection="bottom"
            app:constraint_referenced_ids="icon, title, more" />
        <com.samsung.android.app.musiclibrary.ui.widget.OneUiRecyclerView
            android:id="@id/recycler_view"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/heart_recommend_recycler_view_min_height"
            android:nestedScrollingEnabled="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/barrier" />
        <com.samsung.android.app.musiclibrary.ui.widget.MusicGroup
            android:id="@id/group_contents"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="icon, title, barrier, recycler_view" />
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:id="@id/guideline_start_network"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_begin="45.5dip" />
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:id="@id/guideline_end_network"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_end="45.5dip" />
        <ImageView
            android:id="@id/mobile_data_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:tintMode="src_in"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toTopOf="@id/mobile_data_text"
            app:layout_constraintEnd_toStartOf="@id/guideline_end_network"
            app:layout_constraintStart_toEndOf="@id/guideline_start_network"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed"
            app:tint="@color/heart_recommend_mobile_data_icon" />
        <TextView
            android:textSize="@dimen/heart_recommend_no_network_text"
            android:textColor="@color/heart_recommend_no_network_text"
            android:gravity="center"
            android:id="@id/mobile_data_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/heart_recommend_mobile_data_text_space_top"
            android:textAlignment="center"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toTopOf="@id/mobile_data_button"
            app:layout_constraintEnd_toStartOf="@id/guideline_end_network"
            app:layout_constraintStart_toEndOf="@id/guideline_start_network"
            app:layout_constraintTop_toBottomOf="@id/mobile_data_icon"
            style="@style/Font.NewSEC.400.NoItem" />
        <TextView
            android:textSize="@dimen/heart_recommend_no_network_button_text"
            android:textColor="@color/basics_text_main"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@id/mobile_data_button"
            android:background="@drawable/heart_no_item_button"
            android:focusable="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/heart_recommend_mobile_data_button_space_top"
            android:minWidth="@dimen/heart_recommend_no_network_button_min_width"
            android:minHeight="@dimen/heart_recommend_no_network_button_min_height"
            android:text="@string/mobile_data"
            android:maxLines="1"
            android:singleLine="true"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/guideline_end_network"
            app:layout_constraintStart_toEndOf="@id/guideline_start_network"
            app:layout_constraintTop_toBottomOf="@id/mobile_data_text"
            style="@style/Font.NewSEC.600" />
        <com.samsung.android.app.musiclibrary.ui.widget.MusicGroup
            android:id="@id/group_mobile_data"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="mobile_data_icon, mobile_data_text, mobile_data_button" />
        <ImageView
            android:id="@id/no_network_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:tintMode="src_in"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toTopOf="@id/no_network_text"
            app:layout_constraintEnd_toStartOf="@id/guideline_end_network"
            app:layout_constraintStart_toEndOf="@id/guideline_start_network"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed"
            app:tint="@color/heart_recommend_mobile_data_icon" />
        <TextView
            android:textSize="@dimen/heart_recommend_no_network_text"
            android:textColor="@color/heart_recommend_no_network_text"
            android:gravity="center"
            android:id="@id/no_network_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/heart_recommend_no_network_text_space_top"
            android:textAlignment="center"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toTopOf="@id/no_network_button"
            app:layout_constraintEnd_toStartOf="@id/guideline_end_network"
            app:layout_constraintStart_toEndOf="@id/guideline_start_network"
            app:layout_constraintTop_toBottomOf="@id/no_network_icon"
            style="@style/Font.NewSEC.400.NoItem" />
        <TextView
            android:textSize="@dimen/heart_recommend_no_network_button_text"
            android:textColor="@color/basics_text_main"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@id/no_network_button"
            android:background="@drawable/heart_no_item_button"
            android:focusable="true"
            android:duplicateParentState="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/heart_recommend_no_network_settings_space_top"
            android:minWidth="@dimen/heart_recommend_no_network_button_min_width"
            android:minHeight="@dimen/heart_recommend_no_network_button_min_height"
            android:text="@string/recommend_network_settings"
            android:maxLines="1"
            android:singleLine="true"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/guideline_end_network"
            app:layout_constraintStart_toEndOf="@id/guideline_start_network"
            app:layout_constraintTop_toBottomOf="@id/no_network_text"
            style="@style/Font.NewSEC.600" />
        <com.samsung.android.app.musiclibrary.ui.widget.MusicGroup
            android:id="@id/group_no_network"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="no_network_icon, no_network_text, no_network_button" />
        <androidx.appcompat.widget.SeslProgressBar
            android:id="@id/loading_progress_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0.5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            style="?android:progressBarStyleLarge" />
    </com.samsung.android.app.musiclibrary.ui.widget.OneUiConstraintLayout>
</com.samsung.android.app.musiclibrary.ui.widget.RecyclerViewItemLayout>